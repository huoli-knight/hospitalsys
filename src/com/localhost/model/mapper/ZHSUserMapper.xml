<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.localhost.model.mapper.ZHSUserMapper">

	<select id="findUser" resultType="ZHSUser">
		select user.*
		from user
		where user.DelMark = 1 and user.UseType = 4
	</select>

	
</mapper>